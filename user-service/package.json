    "test:e2e": "jest --config ./test/jest-e2e.json",
    "proto:generate": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./src/app/domain/proto/type --ts_proto_opt=nestJs=true,addGrpcMetadata=true,addNestjsRestParameter=true -I ./src/app/domain/proto ./src/app/domain/proto/*.proto"
  },
